package faith.success;

public class PatternTest {

	public void pattern(int n) {

		// Square Pattern
		
/*		for (int i = 1; i <=n; i++) {
			for (int j = 1; j <= n; j++) {
*/
		
		for (int i = 1; i <=n; i++) {
			
			for (int j = 1; j <= i; j++) {
				System.out.print(j + " ");
			}
			
			System.out.println();
		}
	}

	public static void main(String[] args) {

		PatternTest pt = new PatternTest();
		pt.pattern(5);
	}

}
