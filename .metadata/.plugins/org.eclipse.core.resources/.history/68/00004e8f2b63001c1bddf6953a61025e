package faith.success;

public class PatternTest {

	public void pattern() {

		for (int i = 1; i <=5; i++) {
			for (int j = i; j <= 5; j++) {

				System.out.print(j + " ");
			}
			
			System.out.println();
		}
	}

	public static void main(String[] args) {

		PatternTest pt = new PatternTest();
		pt.pattern();
	}

}
