package faith.success;

public class PatternTest {

	public void pattern(int n) {

		for (int i = 1; i <=n; i++) {
			for (int j = n; j <= n; j++) {

				System.out.print(j + " ");
			}
			
			System.out.println();
		}
	}

	public static void main(String[] args) {

		PatternTest pt = new PatternTest();
		pt.pattern(5);
	}

}
