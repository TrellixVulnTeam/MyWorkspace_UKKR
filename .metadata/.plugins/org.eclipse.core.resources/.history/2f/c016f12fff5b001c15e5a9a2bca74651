package faith.success;

public class PatternTest {

	public void pattern() {

		for (int i = 0; i < 5; i++) {
			for (int j = 5; j > i; j--) {

				System.out.print('*');
			}
			
			System.out.println();
		}
	}

	public static void main(String[] args) {

		PatternTest pt = new PatternTest();
		pt.pattern();
	}

}
